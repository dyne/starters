{
	"name": "{{cookiecutter.project_name}} - webapp",
	"author": {
		"name": "{{cookiecutter.full_name}}",
		"email": "{{cookiecutter.email}}"
	},
	"description": "{{cookiecutter.project_description}}",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=17",
		"pnpm": ">=8"
	},
	"scripts": {
		"predev": "pnpm run definitions",
		"dev": "vite dev",
		"prebuild": "pnpm run definitions",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"test:ui": "playwright test --ui",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"types": "npx -y pocketbase-typegen --db ../admin/pb_data/data.db --out src/modules/pocketbase/types/index.ts",
		"features": "node ./src/lib/features/generateFeaturesList.mjs",
		"roles": "node ./src/lib/organizations/generateRolesList.mjs",
		"definitions": "pnpm types && pnpm export-pocketbase-schema && pnpm features && pnpm roles",
		"serve": "pnpm build && pnpm preview --host",
		"export-pocketbase-config": "node ./src/modules/pocketbase/collections-config/export/index.mjs",
		"convert-pocketbase-schema-to-json-schema": "pnpx vite-node ./src/modules/pocketbase/schema/pb-schema-to-json-schema/index.ts",
		"convert-pocketbase-schema-to-zod-schema": "pnpx vite-node ./src/modules/pocketbase/schema/pb-schema-to-json-schema/toZod.ts"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.9.1",
		"@playwright/test": "^1.45.2",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/debug": "^4.1.12",
		"@types/eslint": "^8.56.10",
		"@types/lodash": "^4.17.7",
		"@typescript-eslint/eslint-plugin": "^7.16.1",
		"@typescript-eslint/parser": "^7.16.1",
		"autoprefixer": "^10.4.19",
		"debug": "^4.3.5",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.42.0",
		"json-to-ts": "^2.1.0",
		"postcss": "^8.4.39",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"svelecte": "^3.17.3",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"svelte-heros-v2": "^0.10.12",
		"svelte-preprocess": "^5.1.4",
		"sveltekit-superforms": "2.17.0",
		"tailwindcss": "^3.4.6",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^5.3.4",
		"vitest": "^1.6.0",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@effect/schema": "^0.68.26",
		"@exodus/schemasafe": "^1.3.0",
		"@inlang/paraglide-sveltekit": "0.8.1",
		"@popperjs/core": "^2.11.8",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"codejar": "^4.2.0",
		"dayjs": "^1.11.13",
		"effect": "^3.8.3",
		"flowbite": "^2.4.1",
		"flowbite-svelte": "^0.44.24",
		"flowbite-svelte-blocks": "^1.1.3",
		"flowbite-svelte-icons": "^1.6.1",
		"formsnap": "^1.0.1",
		"highlight.js": "^11.10.0",
		"json-schema-to-zod": "^2.4.1",
		"lodash": "^4.17.21",
		"nanoid": "^5.0.7",
		"pocketbase": "^0.21.3",
		"svelte-boring-avatars": "^1.2.6",
		"svelte-heros": "^6.0.3",
		"tailwind-merge": "^2.5.2",
		"type-fest": "^4.26.1",
		"zenroom": "^4.32.6"
	}
}
